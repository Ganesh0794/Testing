<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Flow-Callout-1</Name>
            </Step>
            <Step>
                <Name>Key-Value-Map-Operations-1</Name>
            </Step>
            <Step>
                <Name>Lookup-Cache-1</Name>
            </Step>
            <Step>
                <Name>SC_GatePass</Name>
                <Condition>lookupcache.Lookup-Cache-1.cachehit=="false"</Condition>
            </Step>
            <Step>
                <Name>JavaScript-1</Name>
            </Step>
            <Step>
                <Name>Populate-Cache-1</Name>
                <Condition>lookupcache.Lookup-Cache-1.cachehit=="false"</Condition>
            </Step>
            <Step>
                <Name>Assign-Message-1</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <HTTPProxyConnection>
        <BasePath>/cache_testing</BasePath>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>